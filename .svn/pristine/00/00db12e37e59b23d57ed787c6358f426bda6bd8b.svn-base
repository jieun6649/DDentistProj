<script>
// window.onload = function () {
	function addDocument(){
		var docSelected = document.querySelector("#documentChoice");	
		if(docSelected.value == ''){
			docSelected.style.border = '1px solid red';
			return false;
		}else {
			docSelected.style.border = '';
		}
		
		var docCnt = document.querySelector("#docCunt");
		if(docCnt.value == ''){
			docCnt.style.border = '1px solid red';
			return false;
		} else {
			docCnt.style.border = '';
		}
		
	}
// }
</script>

