package com.web.ddentist.vo;

import java.util.Date;
import java.util.List;



//발주

public class PurchaseVO {
	private String purNum;
	private int purCost;
	private Date purDt;
	private String purStatus;
	
	//발주 : 발주 약품 = 1 : N
	private List<PurchaseDrugVO> purchaseDrugList;
	
	public PurchaseVO() {}

	public String getPurNum() {
		return purNum;
	}

	public void setPurNum(String purNum) {
		this.purNum = purNum;
	}

	public int getPurCost() {
		return purCost;
	}

	public void setPurCost(int purCost) {
		this.purCost = purCost;
	}

	public Date getPurDt() {
		return purDt;
	}

	public void setPurDt(Date purDt) {
		this.purDt = purDt;
	}

	public String getPurStatus() {
		return purStatus;
	}

	public void setPurStatus(String purStatus) {
		this.purStatus = purStatus;
	}

	public List<PurchaseDrugVO> getPurchaseDrugList() {
		return purchaseDrugList;
	}

	public void setPurchaseDrugList(List<PurchaseDrugVO> purchaseDrugList) {
		this.purchaseDrugList = purchaseDrugList;
	}

	@Override
	public String toString() {
		return "PurchaseVO [purNum=" + purNum + ", purCost=" + purCost + ", purDt=" + purDt + ", purStatus=" + purStatus
				+ ", purchaseDrugList=" + purchaseDrugList + "]";
	}
	
	
	
	
	
}
